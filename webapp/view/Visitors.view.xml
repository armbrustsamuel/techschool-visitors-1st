<mvc:View controllerName="com.sapvisitors-techschool-1st.controller.Visitors" 
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" 
	displayBlock="true" 
	xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<List id="visitorList" class="sapContrast sapContrastPlus">
				    	<!--https://sapui5.hana.ondemand.com/#/sample/sap.m.sample.NotificationListItem/code/C.controller.js-->
		                <NotificationListItem
							title="Author has accessed the BlueMix website"
							description="This is a tracker for Website visitors."
							showCloseButton="true"
							datetime="None"
							unread="true"
							priority="None"
							close="onItemClose"
							authorName="Author">
						<buttons>
							<Button text="Accept" type="Accept" press="onAcceptPress"/>
							<Button text="Reject" type="Reject" press="onRejectPress"/>
						</buttons>
					</NotificationListItem>
		          </List>
				</content>
				<footer>
				    <Toolbar>
		    		    <ToolbarSpacer/>
		                <!--<Button text="New task" press="onNewTaskClicked" icon="sap-icon://add"/>-->
		            </Toolbar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>